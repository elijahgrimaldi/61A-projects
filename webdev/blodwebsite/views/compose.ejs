<%- include('partials/header'); -%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="exampleFormControlInput1">Title</label>
        <input type="text" class="form-control" name="titleSubmit" id="exampleFormControlInput1">
    </div>
    <div class="form-group">
        <label for="exampleFormControlTextarea1">Post</label>
        <textarea class="form-control" id="exampleFormControlTextarea1" name="bodySubmit" rows="3"></textarea>
    </div>
    <button type="submit" class="btn btn-primary mb-2">Publish</button>
</form>
<%- include('partials/footer'); -%>